#include <iostream>
#include <cmath>
using namespace std;
int main() {
    int n;
    cin >> n;

    if (n <= 20) {
        cout << "n must be greater than 20" << endl;
        return 0;
    }
    int m = ceil(n / 10);
    int arr1[n];
    int arr2[m];
    for (int i = 0; i < n; i++) {
        cin >> arr1[i];
    }

    for (int i = 0; i < m; i++) {
        cin >> arr2[i];
    }

    for (int i = 0; i < m; i++) {
        if (arr2[i] != 0)
            {
            int divisor = arr1[i] / arr2[i];
            int remainder = arr1[i] % arr2[i];
            cout << "Divisor: " << divisor << ", Remainder: " << remainder << endl;
        } else {
            cout << "invalid" << i << endl;
        }
    }

    return 0;
}

